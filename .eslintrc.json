{
  "env": {
    "jest": true,
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
	"linebreak-style": 0,
	"no-tabs": 0,
	"indent": 0,
	"spaced-comment": 0,
	"no-mixed-spaces-and-tabs": 0,
	"quotes": 0,
	"class-methods-use-this": 0,
	"no-use-before-define": 0,
	"no-plusplus": 0,
	"no-cond-assign": 0,
	"eqeqeq": 0,
	"operator-linebreak": 0,
	"space-before-function-paren": 0,
	"prefer-arrow-callback": 0,
	"padded-blocks": 0,
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}
